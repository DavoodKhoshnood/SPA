<h2>List Person</h2>
<table class="table table-bordered" id="list">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Famiy</th>
        <th>Age</th>
        <th>Command</th>
    </tr>

</table>

<script>
    $.getJSON("/Api/People", function (res) {
        $.each(res, function (key, val) {
            var item = "<tr id='tr_"+val.PersonID+"'><td>" +
                val.PersonID + "</td><td>" +
                val.Name + "</td><td>" +
                val.Family + "</td><td>" +
                val.Age + "</td><td> " +
                "<button class='btn btn-sm btn-warning mr-1' onClick='Edit(" + val.PersonID + ")'>" +"Edit</button> " +
                "<button class='btn btn-sm btn-danger mr-1' onClick='Delete(" + val.PersonID + ")'>" +"Delete</button> " +
                "</td ></tr > ";
            $("#list").append(item);
        });
    });
</script>